import java.util.Scanner;
public class Ex001 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scn=new Scanner(System.in);
		String n=scn.next();
		char data1[]=n.toCharArray();
		int len=n.length();
		String m="";
		//char data2[]=new char[len+len-1];
		for(int i=0;i<len;i++){
			m=""+data1[i]+"\t";
			System.out.print(m);
		}

	}

}
